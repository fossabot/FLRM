{
  "plugins": ["plugins/markdown"],
  "recurse": true,
  "source": {
    "includePattern": ".+\\.js(doc|x|t)?$",
    "excludePattern": ""
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
