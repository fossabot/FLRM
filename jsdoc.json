{
  "plugins": ["plugins/markdown"],
  "recurse": true,
  "source": {
    "includePattern": ".+\\.js(doc|x|t)?$",
    "excludePattern": "",
    "include": ["lib/custom","lib/build"],
    "exclude": ["lib/jtech-input"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
